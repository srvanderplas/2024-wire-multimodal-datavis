	file = {Kim et al. - 2019 - A Bayesian Cognition Approach to Improve Data Visu.pdf:/home/susan/Nextcloud/Zotero/storage/D3FZYDRR/Kim et al. - 2019 - A Bayesian Cognition Approach to Improve Data Visu.pdf:application/pdf},
